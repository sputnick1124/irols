<sdf version='1.5'>
  <model name='lezl_cam'>

    <include>
      <uri>model://lezl</uri>
    </include>

    <include>
      <uri>model://camera</uri>
      <pose>0.0 0.0 0 0 1.5708 1.5708</pose>
    </include>

    <joint name="camera_joint" type="revolute">
      <child>camera::link</child>
      <parent>lezl::base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
    </joint>
    

  </model>
</sdf>
